 class Solution {
    public:
    vector<int> twoSum(vector<int>& numbers, int target) {
    vector<int>list;
    for(int i=0;i<numbers.size();i++)
    {
        for(int j=i+1;j<numbers.size();j++)
        {
            if(target==(numbers[i]+numbers[j])){
                list.push_back(i);
            list.push_back(j);
            }
        }
    }
    return list;
}};
